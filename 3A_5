// WAP to find HCF(GCD) of two given numbers.
#include <stdio.h>
#include <stdlib.h>
main()
{
    int number1, number2, GCF=1, i;
    printf("Enter the first number:");
    scanf("%d", &number1);
    printf("Enter the second number:");
    scanf("%d", &number2);
    for(; (GCF<=number1)&&(GCF<=number2); GCF++)
    {
        if((number1%GCF==0)&&(number2%GCF==0))
            i=GCF;
    }
    printf("The GCF of %d and %d is:%d", number1, number2, i);
}
