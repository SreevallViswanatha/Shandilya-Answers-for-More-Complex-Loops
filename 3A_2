// WAP to find all factors of a given number.
#include <stdio.h>
#include <stdlib.h>
main()
{
    int number, factor=1;
    printf("Enter a number:");
    scanf("%d", &number);
    printf("The factors of %d are:", number);
    for(; factor<=number; factor++)
    {
        if(number%factor==0)
        {
            printf("%d ", factor);
        }
    }
}
