// Write a program in C to make such a pattern like right angle triangle with a number which will repeat a number in a row.
#include <stdio.h>
#include <stdlib.h>
main()
{
    int i=1, j, rows;
    printf("Enter the number of rows:");
    scanf("%d", &rows);
    for(; i<=rows; i++)
    {
        j=i;
        for(; j>0; j--)
        {
            printf("%d ", i);
        }
        printf("\n");
    }
}
