// WAP to find twoâ€™s complement of a binary number.
#include <stdio.h>
#include <stdlib.h>
#include<string.h>
main()
{
    char binary[1000];
    int length;
    printf("Enter a binary number:");
    scanf("%s", binary);
    printf("The two's complement of %s is:", binary);
    length=strlen(binary);
    for(; length>0; length--)
    {
        printf("1");
    }
}
